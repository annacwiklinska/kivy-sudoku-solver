<MenuScreen>:
    canvas.before:
        Color:
            rgba: 0.615686, 0.517647, 0.666667, 1
        Rectangle:
            pos: self.pos
            size: self.size

    Button:
        text: "Insert sudoku manually"
        pos_hint: {"x": 0.117994, "y": 0.38}
        size_hint: 0.766962, 0.0866667
        font_size: 50
        background_normal: ''
        background_color: 0.192157, 0.192157, 0.254902, 1
        on_press: root.go_to_manual_input()

    Button:
        text: "Choose from gallery"
        pos_hint: {"x": 0.117994, "y": 0.113333}
        size_hint: 0.766962, 0.0866667
        font_size: 50
        background_normal: ''
        background_color: 0.192157, 0.192157, 0.254902, 1
        on_press: root.go_to_choosing_photo()

    Image:
        source: "public/logo.png"
        allow_stretch: True
        pos_hint: {"x": 0.0589971, "y": 0.451667}
        size_hint: 0.882006, 0.498333
